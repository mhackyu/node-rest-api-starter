{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "func-names": "off",
    "newline-per-chained-call": "warn"
    // "no-unused-vars": "warn"
    // "no-console": "off",
    // "no-process-exit": "off",
    // "object-shorthand": "off",
    // "class-methods-use-this": "off"
  }
}
